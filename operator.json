{
  "name": "K-means",
  "description": "Clusters data points using the K-means algorithm.",
  "tags": [
    "clustering"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/kmeans_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "centers",
      "defaultValue": 10,
      "description": "Number of clusters."
    },
    {
      "kind": "DoubleProperty",
      "name": "iter.max",
      "defaultValue": 10,
      "description": "Max number of iterations."
    },
    {
      "kind": "DoubleProperty",
      "name": "nstart",
      "defaultValue": 1,
      "description": "How many random sets."
    },
    {
      "kind": "StringProperty",
      "name": "seed",
      "defaultValue": "NULL",
      "description": "random seed"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Variable",
            "type": "",
            "description": "Variable identifiers (channels, markers).",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "1..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Observation",
            "type": "",
            "description": "Observation identifiers (cells).",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Measurement",
                "type": "numeric",
                "description": "Measurement values (signal of the channel/marker).",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "OperatorJoinSpec",
        "joinOperators": [
          {
            "kind": "JoinOperator",
            "joinType": "left",
            "leftPair": {
              "kind": "ColumnPair",
              "lColumns": [
                "Observation"
              ],
              "rColumns": [
                "Observation"
              ]
            },
            "rightRelation": {
              "kind": "TableRelation",
              "attributes": [
                {
                  "kind": "Attribute",
                  "name": "cluster",
                  "type": "string"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}